{
  "name": "@vercel/toolbar",
  "version": "0.1.37",
  "description": "Vercel Toolbar",
  "homepage": "https://vercel.com",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "access": "public",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs",
      "default": "./dist/react/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "import": "./dist/next/index.js",
      "require": "./dist/next/index.cjs",
      "default": "./dist/next/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js",
      "require": "./dist/vite/index.cjs",
      "default": "./dist/vite/index.js"
    },
    "./toolbar-object": {
      "types": "./dist/toolbar-object.d.ts",
      "import": "./dist/toolbar-object.js",
      "require": "./dist/toolbar-object.cjs",
      "default": "./dist/toolbar-object.js"
    },
    "./commands": {
      "types": "./dist/commands/index.d.ts",
      "import": "./dist/commands/index.js",
      "require": "./dist/commands/index.cjs",
      "default": "./dist/commands/index.js"
    },
    "./plugins/next": {
      "types": "./dist/plugins/next.d.ts",
      "import": "./dist/plugins/next.js",
      "require": "./dist/plugins/next.cjs",
      "default": "./dist/plugins/next.cjs"
    },
    "./plugins/vite": {
      "types": "./dist/plugins/vite.d.ts",
      "import": "./dist/plugins/vite.js",
      "require": "./dist/plugins/vite.cjs",
      "default": "./dist/plugins/vite.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "react": [
        "dist/react/index.d.ts"
      ],
      "next": [
        "dist/next/index.d.ts"
      ],
      "toolbar-object": [
        "dist/toolbar-object.d.ts"
      ],
      "commands": [
        "dist/commands/index.d.ts"
      ],
      "plugins/next": [
        "dist/plugins/next.d.ts"
      ]
    }
  },
  "dependencies": {
    "@tinyhttp/app": "1.3.0",
    "@vercel/microfrontends": "1.2.0",
    "chokidar": "^3.5.3",
    "execa": "5.1.1",
    "fast-glob": "^3.3.2",
    "find-up": "5.0.0",
    "get-port": "5.1.1",
    "jsonc-parser": "^3.3.1",
    "strip-ansi": "6.0.1"
  },
  "devDependencies": {
    "@babel/core": ">=7.11.0",
    "@babel/runtime": "^7.17.9",
    "@types/node": "^18.16.3",
    "@types/react": "18.2.15",
    "@vercel-private/conformance": "1.7.0",
    "next": ">=11.0.0",
    "react": "19.0.0-rc-38e3b23483-20240529",
    "tsup": "6.7.0",
    "typescript": "4.8.4",
    "vite": "^5.1.6",
    "config": "0.0.0",
    "tsconfig": "0.0.0"
  },
  "peerDependencies": {
    "next": ">=11.0.0",
    "react": ">=17",
    "vite": ">=5"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint-check": "eslint --max-warnings=0 .",
    "publint": "npx publint --yes",
    "type-check": "tsc --noEmit",
    "conformance": "vercel-conformance"
  }
}