{
  "name": "es-mime-types",
  "description": "mime-types rewrite in TypeScript with ESM and CommonJS targets",
  "version": "0.0.16",
  "repository": "https://github.com/talentlessguy/es-mime-types.git",
  "keywords": [
    "http",
    "esm",
    "es",
    "vary",
    "nodejs",
    "mime",
    "mime-types"
  ],
  "engines": {
    "node": ">=12.x"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "talentlessguy <pilll.PL22@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
